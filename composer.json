{
    "name": "spryker-middleware/logger",
    "description": "Logger module",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "spryker/log": "^3.0.0"
    },
    "require-dev": {
        "spryker/calculation": "*",
        "spryker/cart": "*",
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/silex": "*",
        "spryker/testify": "*",
        "spryker/transfer": "*"
    },
    "autoload": {
        "psr-4": {
          "SprykerMiddleware\\": "src/SprykerMiddleware/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600
    }
}
