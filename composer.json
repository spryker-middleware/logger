{
    "name": "spryker-middleware/logger",
    "description": "Logger module",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "spryker/log": "^3.0.0"
    },
    "require-dev": {
        "spryker/calculation": "*",
        "spryker/cart": "*",
        "spryker/code-sniffer": "*",
        "spryker/propel": "*",
        "spryker/silex": "*",
        "spryker/testify": "*",
        "spryker/transfer": "*",
        "spryker/architecture-sniffer": "dev-master",
        "phpmd/phpmd": "^2.4"
    },
    "autoload": {
        "psr-4": {
          "SprykerMiddleware\\": "src/SprykerMiddleware/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600
    },
    "scripts": {
      "cs-check": "phpcs --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v src/ tests/",
      "cs-fix": "phpcbf --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml -v src/ tests/"
    }
}
